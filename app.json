{
  "expo": {
    "name": "Boykotla",
    "slug": "boykotla",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "boykotla",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.gweepcreative.boykotla",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "UIBackgroundModes": ["remote-notification"]
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#FCDCAD"
      },
      "package": "com.gweepcreative.boykotla",
      "permissions": ["RECEIVE_BOOT_COMPLETED", "SCHEDULE_EXACT_ALARM", "USE_EXACT_ALARM"]
    },
    "web": {
      "bundler": "metro",
      "output": "server",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "backgroundColor": "#FCDCAD",
          "image": "./assets/images/splash.png",
          "imageWidth": 720
        }
      ],
      [
        "expo-font",
        {
          "fonts": [
            "node_modules/@expo-google-fonts/sour-gummy/400Regular/SourGummy_400Regular.ttf",
            "node_modules/@expo-google-fonts/sour-gummy/700Bold/SourGummy_700Bold.ttf"
          ]
        }
      ],
      [
        "expo-notifications",
        {
          "icon": "./assets/images/icon.png",
          "color": "#FF6B00"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": "https://boykotla.expo.app"
      },
      "eas": {
        "projectId": "059aa301-7a73-4659-b0f8-2500499fee8c"
      }
    }
  }
}
